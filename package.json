{
  "name": "polygon-clipping",
  "version": "0.6.1",
  "description": "Apply boolean Polygon clipping operations (intersection, union, difference, xor) to your Polygons & MultiPolygons.",
  "main": "main.js",
  "scripts": {
    "build:docs": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "dev": "cross-env webpack-dev-server --mode development --open --hot",
    "test": "jest",
    "test-debug": "node --inspect-brk node_modules/.bin/jest --runInBand"
  },
  "jest": {
    "collectCoverage": true,
    "testEnvironment": "node"
  },
  "keywords": [
    "polygon",
    "clipping",
    "boolean",
    "intersection",
    "union",
    "difference",
    "xor",
    "geometry",
    "martinez"
  ],
  "repository": "github:mfogel/polygon-clipping",
  "author": "Mike Fogel <mike@fogel.ca>",
  "license": "MIT",
  "devDependencies": {
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "jest": "^22.1.4",
    "load-json-file": "^2.0.0",
    "martinez-polygon-clipping": "^0.3.3",
    "raw-loader": "^0.5.1",
    "vue": "^2.5.16",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3"
  },
  "dependencies": {
    "qheap": "^1.4.0",
    "save-dev": "^2.0.0",
    "splaytree": "^0.1.1"
  }
}
